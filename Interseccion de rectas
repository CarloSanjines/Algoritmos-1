// Intersecci√≥n.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "math.h"
#include "conio.h."
#include < iostream >
 using namespace std;

 void main()
 {
	 float A, B, C, D, E, F, x, y;
	 cout<<"Ingrese las variables de la primera ecuacion  \n";
	 cin>>A>>B>>C;

	 cout<<"Ingrese las variables de la segunda ecuacion  \n";
	 cin>>D>>E>>F;

	 if(A != D && B != E)
	 {
		 y=(D*C-A*F)/(B*D-A*E);
		 x=(C-B*y)/A;
		 cout<<"Las rectas se interceptan en las coordenadas:";
		 cout<<"X: "<<x;
		 cout<<" Y:"<<y;
	 }
	 else
	 {
		 cout<<"Las rectas no se interceptan";
	 }
	 getch();
 }
